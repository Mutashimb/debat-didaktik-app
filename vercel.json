{
  "version": 2,
  "builds": [
    {
      "src": "api/config/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/api/staticfiles/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/config/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ]
}